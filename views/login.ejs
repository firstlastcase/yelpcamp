    <title>Yelp Campgrounds - Login</title>
</head>
<body>
    
        <div class="container signup">
            <h2>Log In</h2>
            <form action="/login" method="POST">
                <div class="form-group">
                    <input type="email" name="username" placeholder="email" required>
                </div>
                <div class="form-group">
                    <input type="password" name="password" placeholder="Password" required>
                </div>
    
                <button type="submit" id="submit" class="btn btn-sm btn-outline-success">>  Login  <</button>
                
            </form>
            <a href="/register"> No username? <strong>Sign up</strong> here </a>
            <!--<a type="button" href="/register" class="btn btn-sm btn-outline-secondary">> You don't have a username? no worries, just Sign up here <</a>-->
        </div>


</body>
